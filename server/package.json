{
  "name": "wecar",
  "version": "2.0.0",
  "description": "Back-end do WeCar",
  "main": "dist/server.js",
  "license": "",
  "scripts": {
    "start": "node ./dist/server.js",
    "dev": "ts-node-dev --transpile-only --ignore-watch node_modules -r tsconfig-paths/register src/server.ts",
    "build": "tsc --project tsconfig.json && tscpaths -p tsconfig.json -s ./src -o ./dist",
    "clean": "tsc --build --clean",
    "test": "jest",
    "knex": "knex --knexfile src/knex/knexfile.ts",
    "knex:migrate:make": "npm run knex migrate:make",
    "knex:migrate:latest": "npm run knex migrate:latest",
    "knex:migrate:rollback": "npm run knex migrate:rollback",
    "knex:seed:run": "npm run knex seed:run"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "jsonwebtoken": "^9.0.2",
    "knex": "^2.5.1",
    "knex-paginate": "^3.1.1",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.43",
    "multer": "^1.4.5-lts.1",
    "mysql": "^2.18.1",
    "randomstring": "^1.3.0",
    "tscpaths": "^0.0.9",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.14",
    "@types/express": "^4.17.18",
    "@types/jsonwebtoken": "^9.0.3",
    "@types/node": "^20.8.2",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "dotenv": "^16.3.1",
    "eslint": "^8.49.0",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.2.2"
  }
}
